# Multi-Agent Autonomous Engineering Environment Configuration

# Server Configuration
NODE_ENV=development
PORT=4000
HOST=0.0.0.0

# API Configuration
API_VERSION=v1
API_PREFIX=/api

# AI/LLM Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=4096
AZURE_OPENAI_ENDPOINT=your-azure-endpoint
AZURE_OPENAI_API_KEY=your-azure-api-key
AZURE_OPENAI_DEPLOYMENT=your-deployment-name

# Agent Configuration
MAX_CONCURRENT_AGENTS=10
AGENT_TIMEOUT=300000
AGENT_RETRY_ATTEMPTS=3
AGENT_RETRY_DELAY=1000

# Database Configuration
DATABASE_URL=postgresql://engineer:engineer@localhost:5432/multi_agent_platform
REDIS_URL=redis://localhost:6379
MONGODB_URL=mongodb://localhost:27017/multi_agent_platform

# Authentication & Security
JWT_SECRET=change-this-to-a-random-jwt-secret
JWT_EXPIRES_IN=7d
SESSION_SECRET=change-this-to-a-random-session-secret
ENCRYPTION_KEY=change-this-to-a-32-character-key

# OAuth Configuration
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
GITHUB_CALLBACK_URL=http://localhost:4000/auth/github/callback

# Code Execution Sandbox
SANDBOX_ENABLED=true
SANDBOX_TIMEOUT=30000
SANDBOX_MEMORY_LIMIT=512
DOCKER_SOCKET_PATH=/var/run/docker.sock

# Workflow Orchestration
WORKFLOW_MAX_DEPTH=10
WORKFLOW_TIMEOUT=600000
WORKFLOW_PARALLEL_TASKS=5

# Message Bus Configuration
MESSAGE_BUS_TYPE=redis
MESSAGE_BUS_CHANNEL_PREFIX=agent:
MESSAGE_BUS_RETRY_ATTEMPTS=3

# Context Store
CONTEXT_STORE_TYPE=redis
CONTEXT_TTL=3600
MAX_CONTEXT_SIZE=10000000

# Logging & Monitoring
LOG_LEVEL=info
LOG_FORMAT=json
SENTRY_DSN=your-sentry-dsn
ENABLE_TELEMETRY=true

# Tracing & Debugging
ENABLE_TRACING=true
TRACE_EXPORT_INTERVAL=5000
JAEGER_ENDPOINT=http://localhost:14268/api/traces

# WebSocket Configuration
WS_PORT=4001
WS_PATH=/ws
WS_HEARTBEAT_INTERVAL=30000
WS_MAX_CONNECTIONS=1000

# File Storage
STORAGE_TYPE=local
STORAGE_PATH=./storage
AWS_S3_BUCKET=your-s3-bucket
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key

# Code Repository Integration
GITHUB_TOKEN=your-github-personal-access-token
GITLAB_TOKEN=your-gitlab-token
BITBUCKET_TOKEN=your-bitbucket-token

# DeFi Platform Integration
DEFI_PLATFORM_URL=http://localhost:8000
DEFI_PLATFORM_API_KEY=your-defi-platform-api-key

# Security Settings
ENABLE_SECURITY_VALIDATOR=true
SECURITY_SCAN_TIMEOUT=60000
MAX_CODE_EXECUTION_TIME=30000

# Testing Configuration
TEST_MODE=false
MOCK_AI_RESPONSES=false
MOCK_EXTERNAL_APIS=false

# Rate Limiting
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX_REQUESTS=100

# CORS Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost:5173
CORS_CREDENTIALS=true

# Agent-Specific Settings
INTENT_ROUTER_MODEL=gpt-4
PRODUCT_ARCHITECT_MODEL=gpt-4
CODE_ENGINEER_MODEL=gpt-4
RESEARCH_AGENT_MODEL=gpt-4
TEST_AGENT_MODEL=gpt-4
SECURITY_VALIDATOR_MODEL=gpt-4
AUDIT_AGENT_MODEL=gpt-4

# Performance Settings
MAX_MEMORY_MB=2048
MAX_CPU_PERCENT=80
ENABLE_CACHING=true
CACHE_TTL=3600
